package game;

public class Cell {
	int value;
	int[][] coordinates = new int[2][15];
	//ctor
	public Cell() {
		value = 0;
		for(int i = 0; i < 15; i++) {
				coordinates[0][i] = 0; 
				coordinates[1][i] = 0;
		}
	}
	public void setCell(int x, int y) {
		for(int i = 0; i < 15; i++) {
			coordinates[0][i] = x;
			coordinates[1][i] = y;
		}
	}
	
	
	//public void draw(Graphics g, BufferedImage image) {
		
	//}
}
